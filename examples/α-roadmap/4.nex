fn calc_calories => (int: carb, int: fat, int: prot, int: alc) {
    var: init = 100;
    if (fat > 30) {
        print("HIGH IN FAT");
    } elif (alc > 20) {
        print("HIGH IN ALCOHOL");
    } elif (prot > 30) {
        print("GOOD SOURCE OF PROTEIN");
    } else {
        print("NORMAL FOOD");
    }

    return ((fat * 9) + (carb * 4) + (prot * 4) + (alc * 9));
}

